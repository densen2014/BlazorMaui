        <PackageReference Include="FreeSql.Provider.SqliteCore" Version="3.2.600" />

        <!--<PackageReference Include="FreeSql.Provider.Sqlite" Version="3.2.100" />-->
    </ItemGroup>

    <ItemGroup Condition="'$(Configuration)' != 'DebugFsql'">
    </ItemGroup>

